<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <RootNamespace>Date_Taken_Extractor</RootNamespace>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageId>DateTakenExtractor</PackageId>
        <Authors>ellman12</Authors>
        <Product>Date-Taken-Extractor</Product>
        <Description>Small, fast, simple library for extracting Date Taken metadata from photos and videos.</Description>
        <PackageProjectUrl>https://github.com/ellman12/Date-Taken-Extractor</PackageProjectUrl>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <RepositoryUrl>https://github.com/ellman12/Date-Taken-Extractor</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <NeutralLanguage>en</NeutralLanguage>
        <Company>ellman12</Company>
        <PackageTags>metadata;metadata-extraction;metadata-extractor;datetime-library;datetime;datetaken;date-taken</PackageTags>
        <Copyright></Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MetadataExtractor" Version="2.7.2" />
    </ItemGroup>

</Project>
